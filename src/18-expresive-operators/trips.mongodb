// comparar con $eq y aumentando $and
db.trips.find({
  $expr: {
    $and: [
      {$eq: ["$start station id", "$end station id"]},
      {$gte: ["$tripduration", 1200]} // y sea mayor o =  a 1200
    ]
  }
}).count()
